<COMMAND name="Open" target="GameFindData" filter="" />

<COMMAND name="add">
<RJMETADATA>
  <PROP name="GameID" type="text" value="***Your Game ID goes here. eg - 88FD8F61-B0A8-11d4-8FC8-00D0B7B23FAE***" />
  <PROP name="Version" type="text" value="Demo RGI 1.0" />
  <PROP name="GameName" type="text" value="*** Your Game Title goes here. eg - SimpleSample" />
  <PROP name="GameType" type="Text" value="Demo" />
  <PROP name="MultiID" type="text" value="blank" />
  <PROP name="Data" type="group">
    <PROP name="AUComponent" type="text" value="***When you run the packager, this value is generated. eg - 000000***" />
    <PROP name="Caption" type="text" value="***This is where you place info a short blurb about your game***" /> 
    <PROP name="PublisherID" type="number" value="0" />
    <PROP name="DeveloperID" type="number" value="0" />
    <PROP name="MezzanineURL" type="url" value="http://207.188.30.80/gameconsole/games/demo/mezzanines/AutoTestClient.mez" />
    <PROP name="LaunchType" type="Text" value="Standalone" />
    <PROP name="RealAPISupport" type="Flag" value="True" />
    <PROP name="StartOptions" type="text" value="***Add any required command line parameters here***" />
    <PROP name="FindInRegistry" type="number" value="1" />
    <PROP name="FindKeys" type="group">
    	<PROP name="KeyCount" type="number" value="2" />
    	<PROP name="Key1" type="text" value="Registry;HKEY_CLASSES_ROOT\SOFTWARE\RealNetworks\Games\***GameName goes here***.***AUComponent goes here*** - eg SimpleSample.000000***\*** AU Component version goes here, eg - 1.0.0.1000***;InstallPath" />
	<PROP name="Key2" type="text" value="Registry;HKEY_CLASSES_ROOT\SOFTWARE\RealNetworks\Games\***GameName goes here***.***AUComponent goes here*** - eg SimpleSample.000000***\*** AU Component version goes here, eg - 1.0.0.1000***;Executable" />
      	<PROP name="LaunchCommand" type="text" value="${Key1}${Key2}" />
	<PROP name="LaunchPath" type="text" value="${Key1}" />
	<PROP name="UninstallCommand" type="text" value="<?XZIP?>${Key1}</?XZIP?>" />
	<PROP name="RegMustNotExist" type="text" value="HKEY_CLASSES_ROOT\SOFTWARE\RealNetworks\Games\***GameName goes here***.***AUComponent goes here*** - eg SimpleSample.000000***\*** AU Component version goes here, eg - 1.0.0.1000***;LicenseKey" />
    </PROP>
    <PROP name="DiskSearchInfo" type="Group">
        <PROP name="FileName" type="text" value="***The name of your executable goes here. eg - SimpleSample.exe***" />
        <PROP name="FileVersion" type="text" value="0" />
        <PROP name="FileSize" type="number" value="***size of you exe goes here***" />
    </PROP>
    <PROP name="SystemRequirements" type="group">
      *** Fill these in with appropriate requirements ***
      	<PROP name="OperatingSystem" type="text" value="Windows95" />
      	<PROP name="MinProcessorType" type="text" value="Pentium 90" />
      	<PROP name="MinMemory" type="number" value="16777216" />
	<PROP name="MinDiskSpaceRun" type="number" value="4477419" />
	<PROP name="MinDiskSpaceInstall" type="number" value="104857600" />
	<PROP name="Requires3DHardware" type="flag" value="0" />
      	<PROP name="MinVidMemory" type="number" value="4194304" />
	<PROP name="MinMonitorResolution" type="text" value="640x480" />
      	<PROP name="MinDirectXVersion" type="number" value="0" />
      	<PROP name="SupportsEAXAudio" type="flag" value="0" />
      	<PROP name="SupportsDolbyAudio" type="flag" value="0" />
    </PROP>
  </PROP>
</RJMETADATA>
</COMMAND>


